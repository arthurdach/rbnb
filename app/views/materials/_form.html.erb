<%= simple_form_for [@material] do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :category, collection: Material::CATEGORIES %>
  <%= f.input :price %>

  <%= f.input :photo%>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.button :submit, class: "btn-form btn-primary" %>

<% end %>


